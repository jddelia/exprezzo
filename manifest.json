{
  "manifest_version": 2,

  "name": "Exprezzo",
  "description": "This extension converts image to text",
  "version": "1.0",

  "browser_action": {
   "default_icon": "./assets/icon.png",
   "default_popup": "popup.html"
  },

  "web_accessible_resources": [
    "js/worker.min.js",
    "js/tessearct-core.wasm.js",
    "traineddata/*.traineddata.gz"
  ],

  "permissions": [
   "activeTab",
   "tabs"
   ]
}
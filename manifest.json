{
  "manifest_version": 3,

  "name": "Exprezzo",
  "description": "This extension converts image to text",
  "version": "1.0",

  "action": {
    "default_icon": "./assets/icon.png",
    "default_popup": "popup.html"
  },

  "web_accessible_resources": [{
    "resources": [
      "js/worker.min.js",
      "js/tesseract-core.wasm.js",
      "traineddata/*.traineddata.gz"
    ],
    "matches": ["<all_urls>"]
  }],

  "permissions": [
   "activeTab",
   "tabs"
   ]}
